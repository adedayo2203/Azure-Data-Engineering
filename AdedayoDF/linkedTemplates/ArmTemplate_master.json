{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AdedayoDF"
		},
		"LinkService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkService'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"SqlServer2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer2'"
		},
		"SqlServer3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer3'",
			"defaultValue": "Integrated Security=True;Data Source=DESKTOP-2U79JLL;Initial Catalog=AdventureWorksDW2022"
		},
		"SqlServer3_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer3'"
		},
		"AdedayoKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AdedayoKeyVault.vault.azure.net/"
		},
		"AzureDataLakeStorage1_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureDataLakeStorage1'"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AdedayoKeyVault.vault.azure.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Adedayokeyvault1.vault.azure.net/"
		},
		"AzureKeyVault2_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AdedayoKeyVault.vault.azure.net/"
		},
		"AzureKeyVaultFinal_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AdedayoKeyVault.vault.azure.net/"
		},
		"SqlServer3_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adeniyi"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AdedayoDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkService_connectionString": {
						"value": "[parameters('LinkService_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"SqlServer2_connectionString": {
						"value": "[parameters('SqlServer2_connectionString')]"
					},
					"SqlServer3_connectionString": {
						"value": "[parameters('SqlServer3_connectionString')]"
					},
					"SqlServer3_password": {
						"value": "[parameters('SqlServer3_password')]"
					},
					"AdedayoKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AdedayoKeyVault_properties_typeProperties_baseUrl')]"
					},
					"AzureDataLakeStorage1_sasUri": {
						"value": "[parameters('AzureDataLakeStorage1_sasUri')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVault2_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault2_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVaultFinal_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultFinal_properties_typeProperties_baseUrl')]"
					},
					"SqlServer3_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer3_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}